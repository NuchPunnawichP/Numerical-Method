   "outputs": [],
   "source": [
    "def onep(x):\n",
    "    if x > 1e9:\n",
    "        return \"Divergence\"\n",
    "    \n",
    "    xx = 2 - math.exp(x / 4)\n",
    "\n",
    "    if abs( (xx - x) / xx ) * 100 < 0.05:\n",
    "        return xx\n",
    "    return onep(xx)"
   ]
  },
